{
  "name": "cypress-and-vue-testing-library",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "bulk": "node ./scripts/bulk.js pages/__tests__ form.test.ts; node scripts/bulk.js ./cypress/integration form.js",
    "clean": "rm -rf cypress/**/__dummy__ pages/**/__dummy__",
    "cypress": "cypress run",
    "cypress:measure": "t1=`date '+%s'`; cypress run --config video=false; t2=`date '+%s'`; echo $((t2-t1))",
    "cypress:open": "cypress open",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint": "npm run lint:js",
    "test": "jest",
    "test:measure": "t1=`date '+%s'`; jest; t2=`date '+%s'`; echo $((t2-t1))"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^2.0.0",
    "core-js": "^3.6.5",
    "nuxt": "^2.14.6",
    "vee-validate": "^3.4.5"
  },
  "devDependencies": {
    "@nuxt/types": "^2.14.6",
    "@nuxt/typescript-build": "^2.0.3",
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-config-typescript": "^3.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@testing-library/cypress": "^7.0.3",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/vue": "^5.6.1",
    "@vue/test-utils": "^1.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.5.0",
    "cypress": "^6.1.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "flush-promises": "^1.0.2",
    "jest": "^26.5.0",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1",
    "vue-jest": "^3.0.4"
  }
}
